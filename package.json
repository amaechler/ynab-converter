{
  "name": "ynab-csv-converter",
  "version": "1.0.0",
  "description": "Convert bank CSV to YNAB-compatible format",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc && cp public/* dist/",
    "deploy": "npm run build && gh-pages -d dist",
    "dev": "tsc --watch",
    "format": "prettier --write \"**/*.{ts,js,html,css,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,html,css,json,md}\"",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "eslint": "^9.39.2",
    "gh-pages": "^6.1.1",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3"
  }
}
